<html>
  <head>
    <title>
      HALAMAN TEACHERS
    </title>
  </head>
  <body>
    <header>
      <h1><%= title %></h1>
      </header>
        <table align="center">
          <tr>
            <th>ID</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
          </tr>
            <% for(let i=0;i<teachers.length;i++){ %>
            <tr>
              <td><%= teachers[i].id %></td>
              <td><%= teachers[i].first_name %></td>
              <td><%= teachers[i].last_name %></td>
              <td><%= teachers[i].email %></td>
            </tr>
            <% } %>
        </table>
  </body>
</html>
