<%- include header.ejs %>
  <div class="container-fluid">
    <br>
    <h3>Teachers Data</h3>
    <table class="table">
      <thead>
        <th>Id</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
      </thead>
      <tbody>
        <% for (var i = 0; i < teachers.length; i++) { %>
          <tr>
            <td>
              <%= teachers[i].id %>
            </td>
            <td>
              <%= teachers[i].firstName %>
            </td>
            <td>
              <%= teachers[i].lastName %>
            </td>
            <td>
              <%= teachers[i].email %>
            </td>
          </tr>
          <% } %>
      </tbody>
    </table>
  </div>
  <%- include footer.ejs %>