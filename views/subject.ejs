<html>
  <head>
    <meta charset="utf-8"
    <title></title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
  </head>
  <body>
    <div class="container">
    <center><h1>All Subject Data</h1></center>
    <br>
    <br>
      <table class="table table-bordered">
        <thead>
          <tr>
            <th>Id</th>
            <th>Subject Name</th>
            <th>Teacher</th>
            <th>Actions</th>
          </thead>
          <tbody>
            <%data_subject.forEach(data =>{%>
              <tr>
                <td><%=data.id%></td>
                <td><%=data.subject_name%></td>
                <td><%data.Teachers.forEach((teacher, i) => {%>
                  <%=(i+1)+'. '+ teacher.first_name + ' ' + teacher.last_name%>
                  <%})%>
                </td>
                <td>
                  <a href="/subjects/<%=data.id%>/enrolledstudents">Enrollerd Students</a>
                </td>
              </tr>
              <%})%>
            </tbody>
    </div>
    </table>
  </body>
</html>
