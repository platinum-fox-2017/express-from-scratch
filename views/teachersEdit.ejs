<div>
    <button onclick="location.href ='/';">Dashboard</button>
    <button onclick="location.href ='/students';">Students</button>
    <button onclick="location.href ='/teachers';">Teachers</button>
    <button onclick="location.href ='/subjects';">Subjects</button>
    <button onclick="location.href ='/users';">User</button>
    <button onclick="location.href ='/';">Logout</button>
</div>

<div>
  <form method="post">
    First Name:<br>
    <input type="text" value="<%=teacher.firstName%>" name="firstName"><br>
    Last Name:<br>
    <input type="text" value="<%=teacher.lastName%>" name="lastName"><br>
    Email:<br>
    <input type="text" value="<%=teacher.email%>" name="email"><br>
    Subject:<br>
    <select name="SubjectId">
        <%for(let i in subject) {%>
            <option value="<%=subject[i].id%>" <%=subject[i].id===teacher.SubjectId?`selected="selected"`:'';%>><%=subject[i].name%></option>
        <%}%>
    </select>
    <input type="submit" value="Change!"/>
  </form>
</div>