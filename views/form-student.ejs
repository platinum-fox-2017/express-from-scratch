<html>
    <head>
        <title><%= title %></title>
    </head>
    <body>
        <div id="header">
            <div class="logo"></div>
            <div class="menu">
                <ul>
                    <li><a href="/teachers">Teacher</a></li>
                    <li><a href="/students">Student</a></li>
                    <li><a href="/subjects">Subject</a></li>
                </ul>
            </div>
        </div>
        <h1><%= heading %></h1>
        <form action="<%= formAction %>" method="post">
            <input type="hidden" value="<%= id %>" name="id">
            <input type="text" value="<%= formValue_fname %>" name="fname" placeholder="First Name.."><br>
            <input type="text" value="<%= formValue_lname %>" name="lname" placeholder="Last Name.."><br>
            <input type="text" value="<%= formValue_email %>" name="email" placeholder="Email.."><br>
            <input type="submit">

        </form>
    </body>
</html>