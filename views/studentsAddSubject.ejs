</div>

<div>
    <button onclick="location.href ='/add';">Dashboard</button>
    <button onclick="location.href ='/students';">Students</button>
    <button onclick="location.href ='/teachers';">Teachers</button>
    <button onclick="location.href ='/subjects';">Subjects</button>
    <button onclick="location.href ='/users';">User</button>
    <button onclick="location.href ='/';">Logout</button>
</div>

<div>
  <form method="post">
    First Name:<br>
    <input type="text" value="<%=student.firstName%>" name="firstName" readonly><br>
    Last Name:<br>
    <input type="text" value="<%=student.lastName%>" name="lastName" readonly><br>
    Email:<br>
    <input type="text" value="<%=student.email%>" name="email" readonly><br>
    Adding Subject<br>
    <select name="SubjectId">
      <%for(let i in subjects) {%>
        <option value="<%=subjects[i].id%>"><%=subjects[i].name%></option>
      <%}%>
    </select><br>
    <input type="submit" value="Add Student Subject"/>
  </form>
</div>