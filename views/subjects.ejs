<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
  </head>
  <body>
    <div class="container">
    <table class="table">
      <h2>Subjects Data</h2>
      <thead>
        <tr>
          <th>No</th>
          <th>Subject Name</th>
          <th>Teachers</th>
          <th>Actions</th>
        </tr>
      </thead>
      <tbody>
        <%subjects.forEach((data, i)=>{%>
          <tr>
            <td><%=i+1%></td>
            <td><%=data.subject_name%></td>
            <td><%data.Teachers.forEach((teacher, i) => {%>
              <%=i+1%>. <%=teacher.first_name%> <%=teacher.last_name%></br>
            <%})%>
            </td>
            <td>
              <a href="/subjects/<%=data.id%>/enrolledstudents">Enrolled Students</a>
            </td>
          </tr>
        <%})%>
      </tbody>
    </table>
  </div>
  </body>
</html>
