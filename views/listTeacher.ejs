<body>
    <h1>List Teacher</h1>

    <table border='1'>
        <tr>
          <td>No.</td>
          <td>First Name</td>
          <td>Last Name</td>
          <td>Email</td>
          <td colspan=2><center>Action</center></td>
        </tr>
      <% for(let i in data) {%>
        <tr>
          <td><%= Number(i)+1 %></td>
          <td><%= data[i].first_name %></td>
          <td><%= data[i].last_name %></td>
          <td><%= data[i].email %></td>
          <td><a href="/teachers/update/<%= data[i].id %>">Update Teacher</a></td>
          <td><a href="/teachers/delete/<%= data[i].id %>" onclick="return confirm('delete this teachers ?');">Delete Student</a></td>
        </tr>
      <% }%>
    </table>
    <a href="/teachers/add">Add Teacher</a>
    <a href="/">Back</a>
</body>
